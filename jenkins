pipeline {
    agent any
    stages {
        stage('git repo & clean') {
            steps {
                bat "rmdir  /s /q SpringBootApplication"
                bat "git clone https://github.com/jeyanagarjun/SpringBootApp.git"
                bat "mvn clean -f SpringBootApplication"
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f SpringBootApplication"
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f SpringBootApplication"
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f SpringBootApplication"
            }
        }
    }
}
